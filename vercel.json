{
    "version": 2,
    "name": "voicexai-frontend",
    "builds": [
        {
            "src": "frontend/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "frontend/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    },
    "functions": {
        "frontend/app.py": {
            "maxDuration": 30
        }
    }
}